#include <stdio.h>

int main() {
    int n;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int arr[n];
    int seen[n];          

    // initialize seen[] to 0
    for(int i = 0; i < n; i++)
        seen[i] = 0;

    printf("Enter array elements:\n");

    int repeated = -1;

    for(int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);

        // check if already seen
        if(seen[arr[i]] == 1) {
            repeated = arr[i];
            break;   
        }

        seen[arr[i]] = 1;   
    }

    printf("Repeated element is: %d\n", repeated);

    return 0;
}
