#include <stdio.h>
int main() {
    int a, b, sum, quotient, product, diff;
    printf(" enter first number: ");
    scanf("%d", &a);
    printf(" enter second number: ");
        scanf("%d", &b);
sum = a + b ;
diff= a - b ;
product = a * b ;
quotient = a/b;

printf (" sum of given numbers is %d \n", sum );
printf (" diffrence of given numbers is %d \n", diff );
printf ("  product of given numbers is %d \n", product );
printf (" quotient  of given numbers is %d \n", quotient );
return 0;

}
