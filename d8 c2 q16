#include <stdio.h>
  int main() {
    int a, b, c;
    printf("Enter three numbers with space: ");
    scanf("%d %d %d", &a, &b, &c);

    if (a == b && b == c) {
        printf("All three numbers are equal ");
    }
    else if (a >= b && a >= c) {
        printf("%d is the largest number", a);
    }
    else if (b >= a && b >= c) {
    printf("%d is the largest number", b);
    }
    else {printf("%d is the largest number", c);
    } return 0;
}
