#include <stdio.h>

int main() {
    int n, target;
    printf("Enter size of array: ");
    scanf("%d", &n);

    int a[n];
    printf("Enter elements (sorted): ");
    for (int i = 0; i < n; i++)
        scanf("%d", &a[i]);

    printf("Enter target: ");
    scanf("%d", &target);

    int first = -1, last = -1;

    for (int i = 0; i < n; i++) {
        if (a[i] == target) {
            if (first == -1) first = i;
            last = i;
        }
    }

    printf("%d,%d\n", first, last);

    return 0;
}
