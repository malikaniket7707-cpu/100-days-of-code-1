#include <stdio.h>
#include <string.h>

int main() {
    char s[1000], t[1000];
    printf("Enter first string: ");
    scanf("%s", s);
    printf("Enter second string: ");
    scanf("%s", t);

    int lenS = strlen(s);
    int lenT = strlen(t);

    if (lenS != lenT) {
        printf("Not Anagram");
        return 0;
    }

    int freq[26] = {0};

    // Count frequency of each character in s
    for (int i = 0; i < lenS; i++)
        freq[s[i] - 'a']++;

    // Subtract frequency using t
    for (int i = 0; i < lenT; i++)
        freq[t[i] - 'a']--;

    // Check if all counts are zero
    for (int i = 0; i < 26; i++) {
        if (freq[i] != 0) {
            printf("Not Anagram");
            return 0;
        }
    }

    printf("Anagram");
    return 0;
}
