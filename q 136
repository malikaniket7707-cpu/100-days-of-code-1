#include <stdio.h>
#include <string.h>

enum Operation {
    ADD,
    SUBTRACT,
    MULTIPLY,
    INVALID
};

enum Operation getOperation(char op[]) {
    if (strcmp(op, "ADD") == 0) return ADD;
    if (strcmp(op, "SUBTRACT") == 0) return SUBTRACT;
    if (strcmp(op, "MULTIPLY") == 0) return MULTIPLY;
    return INVALID;
}

int main() {
    char op[10];
    int num1, num2;

    printf("Enter operation and two numbers (e.g., ADD 10 20): ");
    scanf("%s %d %d", op, &num1, &num2);

    enum Operation choice = getOperation(op);

    switch (choice) {
        case ADD:
            printf("%d\n", num1 + num2);
            break;
        case SUBTRACT:
            printf("%d\n", num1 - num2);
            break;
        case MULTIPLY:
            printf("%d\n", num1 * num2);
            break;
        default:
            printf("Invalid operation\n");
    }

    return 0;
}

