#include <stdio.h>
#include <string.h>

enum UserRole {
    ADMIN,
    USER,
    GUEST,
    INVALID
};

enum UserRole getRole(char role[]) {
    if (strcmp(role, "ADMIN") == 0) return ADMIN;
    if (strcmp(role, "USER") == 0) return USER;
    if (strcmp(role, "GUEST") == 0) return GUEST;
    return INVALID;
}

int main() {
    char roleInput[10];
    
    printf("Enter your role (ADMIN, USER, GUEST): ");
    scanf("%s", roleInput);

    enum UserRole role = getRole(roleInput);

    switch (role) {
        case ADMIN:
            printf("Welcome, Admin! You have full access.\n");
            break;
        case USER:
            printf("Welcome, User! You have limited access.\n");
            break;
        case GUEST:
            printf("Welcome, Guest! You have guest access.\n");
            break;
        default:
            printf("Invalid role entered.\n");
    }

    return 0;
}
