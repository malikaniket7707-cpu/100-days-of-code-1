#include <stdio.h>
#include <string.h>

typedef enum {
    MALE,
    FEMALE
} Gender;

struct Person {
    Gender gender;
};

int main() {
    struct Person p;
    char input[10];

    printf("Enter gender (MALE/FEMALE): ");
    scanf("%s", input);

    if (strcmp(input, "MALE") == 0)
        p.gender = MALE;
    else if (strcmp(input, "FEMALE") == 0)
        p.gender = FEMALE;
    else {
        printf("Invalid gender");
        return 0;
    }

    if (p.gender == MALE)
        printf("male");
    else
        printf("female");

    return 0;
}
