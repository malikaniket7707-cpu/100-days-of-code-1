#include <stdio.h>
#include <string.h>

struct Student {
    char name[21];
    int roll;
    int marks;
};

void printStu(struct Student s) {
    printf("Name: %s | Roll: %d | Marks: %d\n", s.name, s.roll, s.marks);
}

int main() {
    struct Student s;

    printf("Enter your name: ");
    scanf("%s", &s.name);

    printf("Enter your roll number: ");
    scanf("%d", &s.roll);

    printf("Enter your marks: ");
    scanf("%d", &s.marks);

    printStu(s);

    return 0;
}
