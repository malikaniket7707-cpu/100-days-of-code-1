#include <stdio.h>

int main() {
    int n, first, last, digits = 1, temp, result;
    printf("Enter a number: ");
    scanf("%d", &n);

    if (n >= 0 && n <= 9) {
        printf("Number after swapping first and last digit: %d", n);
        return 0;
    }

    temp = n;
    last = temp % 10;

    while (temp >= 10) {
        temp /= 10;
        digits *= 10;
    }
    first = temp;

    result = last * digits + (n % digits - last) + first;

    printf("Number after swapping first and last digit: %d", result);
    return 0;
}
