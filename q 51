#include <stdio.h>

int main() {
    int i, j, start;
    int rows = 5;

    for (i = rows; i >= 1; i--) {
        // Print leading spaces
        for (j = 1; j < i; j++) {
            printf(" ");
        }
        // Print numbers from i to 5
        for (start = i; start <= rows; start++) {
            printf("%d", start);
        }
        printf("\n");
    }

    return 0;
}
