#include <stdio.h>

int main() {
    int r, c, count = 1;
    printf("Enter number of rows: ");
    scanf("%d", &r);
    printf("Enter number of columns: ");
    scanf("%d", &c);

    int a[r][c], sum = 0;

    for (int i = 0; i < r; i++) {
        for (int j = 0; j < c; j++) {
            printf("Enter element %d: ", count);
            scanf("%d", &a[i][j]);
            sum += a[i][j];
            count++;
        }
    }

    printf("Sum of all elements = %d\n", sum);

    return 0;
}
