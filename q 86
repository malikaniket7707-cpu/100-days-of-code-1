#include <stdio.h>

int main() {
    char str[1000];
    int i = 0, isPalindrome = 1;

    printf("Enter a string: ");
    scanf("%[^\n]", str);

    while (str[i] != '\0') i++;
    int len = i;

    for (int j = 0; j < len / 2; j++) {
        if (str[j] != str[len - 1 - j]) {
            isPalindrome = 0;
            break;
        }
    }

    if (isPalindrome)
        printf("Palindrome\n");
    else
        printf("Not Palindrome\n");

    return 0;
}
