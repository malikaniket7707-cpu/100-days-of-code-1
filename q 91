#include <stdio.h>

int main() {
    char str[1000];
    int i = 0, j = 0;

    printf("Enter a string: ");
    scanf("%[^\n]", str);

    while (str[i] != '\0') {
        char c = str[i];

        if (c!='a' && c!='e' && c!='i' && c!='o' && c!='u' &&
            c!='A' && c!='E' && c!='I' && c!='O' && c!='U') {
            str[j++] = str[i];
        }
        i++;
    }

    str[j] = '\0';

    printf("String without vowels: %s", str);

    return 0;
}
